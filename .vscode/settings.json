{
    "eslint.options": {    //指定vscode的eslint所处理的文件的后缀
        "extensions": [
            ".js",
            ".ts",
            ".tsx",
            ".vue"
        ]
    },

    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],

    "vetur.validation.template": false,

    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    }
}